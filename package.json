{
  "name": "agiml-ts",
  "version": "1.0.0",
  "description": "A TypeScript LLM client with AGIML support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "ts-node scripts/run-tests.ts",
    "example:basic": "ts-node examples/basic.ts",
    "example:agiml": "ts-node examples/agiml.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "llm",
    "ai",
    "agiml",
    "typescript",
    "openai",
    "anthropic",
    "middleware"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.7",
    "cohere-ai": "^7.14.0",
    "dotenv": "^16.4.5",
    "openai": "^4.72.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  }
}
